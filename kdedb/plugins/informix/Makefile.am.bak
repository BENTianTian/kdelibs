INCLUDES = -I$(top_builddir)/lib -I$(INFORMIXDIR)/incl -I$(INFORMIXDIR)/incl/esql $(all_includes)

METASOURCES = AUTO


lib_LTLIBRARIES = libkdb_informix.la

libkdb_informix_la_LDFLAGS = -L$(top_builddir)/lib/kdbui -L$(top_builddir)/lib/kdbcore $(all_libraries) $(KDE_PLUGIN)

libkdb_informix_la_LIBADD = $(LIB_KDBCORE) $(LIB_KDBUI) $(LIB_KDEUI) -L$(INFORMIXDIR)/lib -L$(INFORMIXDIR)/lib/esql -lifsql -lifasf -lifgen -lifos -lifgls -lc -lm -ldl -lcrypt $(INFORMIXDIR)/lib/esql/checkapi.o -lifglx

datadir = $(kde_servicesdir)
data_DATA = kdb_informix.desktop 
