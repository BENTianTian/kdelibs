#! /bin/sh
# preinst script for kdelibs
#
# see: dh_installdeb(1)

set -e

case "$1" in
    install)
    ;;
    upgrade)
      if [ -e /etc/kde2/applnk -a ! -L /etc/kde2/applnk ]; then
          if [ -d /usr/share/applnk ]; then
            cp -R /etc/kde2/applnk/* /usr/share/applnk/ >/dev/null 2>&1
            rm -rf /etc/kde2/applnk >/dev/null 2>&1
          else
            mv /etc/kde2/applnk /usr/share/applnk >/dev/null 2>&1
          fi
      fi
    ;;

    abort-upgrade)
    ;;

    *)
        echo "preinst called with unknown argument \`$1'" >&2
        exit 0
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#KDEHELPER#
#DEBHELPER#

exit 0


