#! /bin/sh
# postinst script for kdeprint
#
# see: dh_installdeb(1)

set -e
case "$1" in
    configure)
      /sbin/ldconfig
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)

    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 0
    ;;
esac

#DEBHELPER#

exit 0
