# These are the rules used by makekdedoc to generate the kdelibs
# documentation with KDOC. Please update them if I forget any
# dependencies.
#
# -Taj (Apr 1999)
#
# $Id$

kde_MODULES = qk dcop kdecore kdeui kimgio kio kfile khtml kjs kdejava kformula kspell kab kparts mediatool interfaces

qk_FILES = *.h
qk_LIBS = -lqt

dcop_FILES = *.h
dcop_LIBS = -lqk -lqt

kdecore_FILES = *.h
kdecore_LIBS = -lqk -ldcop -lqt

kdeui_FILES = *.h
kdeui_LIBS = -lkdecore -lqk -ldcop -lqt

kab_FILES = kabapi.h
kab_LIBS = -lkdecore -lkdeui -lqt

kfile_FILES= *.h
kfile_LIBS= -lkio -lkdecore -lkdeui -lqt

kspell_FILES = kspell.h
kspell_LIBS = -lkdecore -lkdeui -lqt

khtml_FILES = *.h
khtml_LIBS = -lkdecore -lkdeui -lkimgio -lqt

kjs_FILES = *.h
kjs_PATH = khtml/kjs
kjs_LIBS = -lkdecore -lkdeui -lkimgio -lqt

kdejava_FILES = *.h
kdejava_PATH = khtml/java
kdejava_LIBS = -lkdecore -lkdeui -lkimgio -lqt

kio_FILES = *.h
kio_LIBS = -lkdecore -lkdeui -ldcop -lqt

kimgio_FILES = kimgio.h
kimgio_LIBS = -lqt

kformula_FILES = *.h
kformula_LIBS = -lkdecore -lkdeui -lqt

kparts_FILES = *.h
kparts_LIBS = -lkdeui -lkdecore -lqt

mediatool_FILES = *.h
mediatool_LIBS = -lqt

interfaces_FILES = *.h
interfaces_LIBS = -lkparts -lqt
