# These are the rules used by makekdedoc to generate the kdelibs
# documentation with KDOC. Please update them if I forget any
# dependencies.
#
# -Taj (Apr 1999)

kde_MODULES = dcop kdecore kdeui kimgio kio kfile khtml kjs kformula kspell kab kparts

dcop_FILES = *.h
dcop_LIBS = -lqt

kdecore_FILES = *.h
kdecore_LIBS = -lqt

kdeui_FILES = *.h
kdeui_LIBS = -lkdecore -lqt

kab_FILES = kabapi.h
kab_LIBS = -lkdecore -lkdeui -lqt

kfile_FILES= *.h
kfile_LIBS= -lkdecore -lkdeui -lqt

kspell_FILES = kspell.h
kspell_LIBS = -lkdecore -lkdeui -lqt

khtml_FILES = *.h
khtml_LIBS = -lkdecore -lkdeui -lkimgio -lqt

kjs_FILES = *.h
kjs_PATH = khtml/kjs
kjs_LIBS = -lkdecore -lkdeui -lkimgio -lqt

kio_FILES = *.h
kio_LIBS = -lkdecore -lkdeui -lqt

kimgio_FILES = kimgio.h
kimgio_LIBS = -lqt

kformula_FILES = *.h
kformula_LIBS = -lkdecore -lkdeui -lqt

kparts_FILES = *.h
kparts_LIBS = -lkdeui -lkdecore -lqt
