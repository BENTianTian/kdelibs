# These are the rules used by makekdedoc to generate the kdelibs
# documentation with KDOC. Please update them if I forget any
# dependencies.
#
# -Taj (Apr 1999)
#
# $Id$

kde_MODULES = dcop kdecore kdeui kimgio kio kfile kparts khtml kjs kdejava kformula kspell kab libkmid kwrite

dcop_FILES = *.h
dcop_LIBS = -lkde-qt-addon -lqt

kdecore_FILES = *.h
kdecore_LIBS = -lkde-qt-addon -ldcop -lqt

kdeui_FILES = *.h
kdeui_LIBS = -lkdecore -lkde-qt-addon -ldcop -lqt

kab_FILES = kabapi.h
kab_LIBS = -lkdecore -lkdeui -lqt

kfile_FILES= *.h
kfile_LIBS= -lkio -lkdecore -lkdeui -lqt

kspell_FILES = *.h
kspell_LIBS = -lkdecore -lkdeui -lqt

khtml_FILES = *.h
khtml_LIBS = -lkparts -lkdecore -lkdeui -lkimgio -lqt

kjs_FILES = kjs.h object.h function.h operations.h ustring.h
kjs_LIBS = -lqt

kdejava_FILES = *.h
kdejava_PATH = khtml/java
kdejava_LIBS = -lkdecore -lkdeui -lkimgio -lqt

kio_FILES = *.h
kio_LIBS = -lkdecore -lkdeui -ldcop -lqt

kimgio_FILES = kimageio.h
kimgio_LIBS = -lqt

kformula_FILES = *.h
kformula_LIBS = -lkdecore -lkdeui -lqt

kparts_FILES = *.h
kparts_LIBS = -lkdeui -lkdecore -lqt

libkmid_FILES = *.h
libkmid_LIBS = -lqt

kwrite_FILES = kwrite.h kwrite_part.h kwrite_factory.h kwrite_view.h kwrite_doc.h highlight.h
kwrite_LIBS = -lkparts -lkdeui -lqt
