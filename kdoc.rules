# These are the rules used by makekdedoc to generate the kdelibs
# documentation with KDOC. Please update them if I forget any
# dependencies.
#
# -Taj (Apr 1999)
#
# $Id$

kde_MODULES = dcop kdefx kdecore kdeui kio kded kssl kfile kparts kjs kdeprint khtml kdejava kabc libkmid interfaces kutils

dcop_FILES = *.h
dcop_LIBS = -lqt

kdefx_FILES = *.h
kdefx_LIBS = -lqt

kdecore_FILES = k*.h net*.h
kdecore_LIBS = -lkdefx -ldcop -lqt

kdeui_FILES = *.h
kdeui_LIBS = -lkdecore -lkdefx -ldcop -lqt

kabc_FILES = address.h addressbook.h addressee.h addresseedialog.h distributionlist.h distributionlisteditor.h format.h geo.h phonenumber.h resource.h simpleformat.h stdaddressbook.h timezone.h vcardformat.h
kabc_LIBS = -lkdecore -lkdeui -lqt

kfile_FILES= *.h
kfile_PATH = kio/kfile
kfile_LIBS= -lkio -lkdecore -lkdeui -lkdefx -lqt

kjs_FILES = value.h types.h object.h interpreter.h operations.h ustring.h
kjs_LIBS = -lqt

kdeprint_FILES = kprinter.h kprintdialogpage.h kpreloadobject.h
kdeprint_LIBS = -lkdefx -lkdeui -lkdecore -lqt

khtml_FILES = *.h dom/*.h
khtml_LIBS = -ldcop -lkjs -lkio -lkparts -lkdecore -lkdeui -lkdefx -lkdeprint -lqt

kdejava_FILES = *.h
kdejava_PATH = khtml/java
kdejava_LIBS = -lkparts -lkdecore -lkdeui -lkdefx -lqt

kio_FILES = *.h
kio_PATH = kio/kio
kio_LIBS = -lkdecore -lkdeui -lkdefx -ldcop -lqt

kded_FILES = kdedmodule.h
kded_PATH = kded
kded_LIBS = -lkio -lkdecore -lkdeui -lkdefx -ldcop -lqt

kssl_FILES = *.h
kssl_PATH = kio/kssl
kssl_LIBS = -lkdeui -lkdefx -ldcop -lqt

kparts_FILES = *.h
kparts_LIBS = -lkio -lkdeui -lkdefx -lkdecore -lqt

libkmid_FILES = *.h
libkmid_LIBS = -lqt

kutils_FILES = *.h
kutils_LIBS = -lkdeui -lkdecore -lqt

interfaces_FILES = */*.h
interfaces_LIBS = -lkparts -lkdeui -lkdecore -ldcop -lqt
