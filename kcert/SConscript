#! /usr/bin/env python
#generated from Makefile.am by am2bksys.py
Import('env')

kcertpart_sources = """
kcertpart.cc
"""

includes = '#/kio/kssl '

obj = env.kdeobj('module')
obj.target = 'kcertpart'
obj.source = kcertpart_sources
obj.includes = includes
obj.libs += ' kdeui kparts'
obj.libpaths += ' ../kdeui ../kparts '
obj.uselib = 'QT QTCORE QTGUI KDE4 KPARTS OPENSSL QT3SUPPORT'
obj.libs += ' kdecore kdeui kio kparts'
obj.libpaths += ' ../kdecore ../kdeui ../kio ../kparts '
if env['WINDOWS']:
	obj.libs += ' kdewin32 '
	obj.libpaths += ' ../win '
obj.execute()

env.bksys_insttype('KDESERV', '', 'kcertpart.desktop')

env.bksys_insttype('KDEAPPS', 'kcertpart', 'kcertpart.rc')


