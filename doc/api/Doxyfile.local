## In kdelibs, everything is supposed to be documented.
WARN_IF_UNDOCUMENTED = YES
DOCUMENT_INTERNAL = YES

## Predefined macros, to remove all the _EXPORT things
## from function definitions.
PREDEFINED = KDECORE_EXPORT="" \
	KDEUI_EXPORT="" \
	KDEFX_EXPORT="" \
	KDE_DEPRECATED="" \
	Q_OS_UNIX="" \
	Q_SLOTS="slots" \
	Q_SIGNALS="signals"

