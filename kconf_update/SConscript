#! /usr/bin/env python
#generated from Makefile.am by am2bksys.py
Import('env')

kconf_update_sources = """
kconf_update.cpp
"""

obj = env.kdeinitobj(env)
obj.target   = 'kconf_update'
obj.source = kconf_update_sources
obj.uselib   = 'QT QTCORE KDE4 KDECORE '
obj.libpaths = '../kdecore ../dcop'
obj.libs     = 'kdecore DCOP'
obj.instdir  = env['KDEBIN']
obj.includes = '../kded '
if env['WINDOWS']:
	obj.libs += ' kdewin32 '
	obj.libpaths += ' ../win '
obj.execute()

