#include "value.h"

static inline JSValue* Convert_boolToValue(ExecState* exec, bool in) {
    return jsBoolean(in);
}

static inline uint32_t Convert_boolToUInt32(ExecState* exec, bool in) {
    return in ? 1 : 0;
}

static inline bool Convert_valueToBool(ExecState* exec, JSValue* in) {
    return in->toBoolean(exec);
}

static inline bool Convert_valueToUInt32(ExecState* exec, JSValue* in) {
    return in->toUInt32(exec);
}

static inline JSValue* Convert_UInt32ToValue(ExecState* exec, uint32_t in) {
    return jsNumber(in);
}

static inline bool Convert_UInt32ToBool(ExecState* exec, uint32_t in) {
    return bool(in);
}

runBlock(...) {
    Addr pc = 0; //Use local...
    exec->setPc(&pc);

    while (true) {
        @generate
    }
}

