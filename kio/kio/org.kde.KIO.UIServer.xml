<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.KIO.UIServer">
    <signal name="actionPerformed">
      <arg name="actionId" type="i" direction="out"/>
    </signal>
    <method name="newJob">
      <arg name="appServiceName" type="s" direction="in"/>
      <arg name="showProgress" type="b" direction="in"/>
      <arg name="internalAppName" type="s" direction="in"/>
      <arg name="jobIcon" type="s" direction="in"/>
      <arg name="appName" type="s" direction="in"/>
      <arg name="jobId" type="i" direction="out"/>
    </method>
    <method name="jobFinished">
      <arg name="id" type="i" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="newAction">
      <arg name="jobId" type="i" direction="in"/>
      <arg name="actionText" type="s" direction="in"/>
      <arg name="actionId" type="i" direction="out"/>
    </method>
    <method name="editAction">
      <arg name="actionId" type="i" direction="in"/>
      <arg name="actionText" type="s" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="enableAction">
      <arg name="actionId" type="i" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="disableAction">
      <arg name="actionId" type="i" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="removeAction">
      <arg name="actionId" type="i" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="totalSize">
      <arg name="id" type="i" direction="in"/>
      <arg name="size" type="t" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="totalFiles">
      <arg name="id" type="i" direction="in"/>
      <arg name="files" type="u" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="totalDirs">
      <arg name="id" type="i" direction="in"/>
      <arg name="dirs" type="u" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="processedSize">
      <arg name="id" type="i" direction="in"/>
      <arg name="size" type="t" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="processedFiles">
      <arg name="id" type="i" direction="in"/>
      <arg name="files" type="u" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="processedDirs">
      <arg name="id" type="i" direction="in"/>
      <arg name="dirs" type="u" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="percent">
      <arg name="id" type="i" direction="in"/>
      <arg name="ipercent" type="u" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="speed">
      <arg name="id" type="i" direction="in"/>
      <arg name="bytesPerSecond" type="s" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="infoMessage">
      <arg name="id" type="i" direction="in"/>
      <arg name="msg" type="s" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="progressInfoMessage">
      <arg name="id" type="i" direction="in"/>
      <arg name="msg" type="s" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="copying">
      <arg name="id" type="i" direction="in"/>
      <arg name="from" type="s" direction="in"/>
      <arg name="to" type="s" direction="in"/>
      <arg name="res" type="b" direction="out"/>
    </method>
    <method name="moving">
      <arg name="id" type="i" direction="in"/>
      <arg name="from" type="s" direction="in"/>
      <arg name="to" type="s" direction="in"/>
      <arg name="res" type="b" direction="out"/>
    </method>
    <method name="deleting">
      <arg name="id" type="i" direction="in"/>
      <arg name="url" type="s" direction="in"/>
      <arg name="res" type="b" direction="out"/>
    </method>
    <method name="transferring">
      <arg name="id" type="i" direction="in"/>
      <arg name="url" type="s" direction="in"/>
      <arg name="res" type="b" direction="out"/>
    </method>
    <method name="creatingDir">
      <arg name="id" type="i" direction="in"/>
      <arg name="url" type="s" direction="in"/>
      <arg name="res" type="b" direction="out"/>
    </method>
    <method name="stating">
      <arg name="id" type="i" direction="in"/>
      <arg name="url" type="s" direction="in"/>
      <arg name="res" type="b" direction="out"/>
    </method>
    <method name="mounting">
      <arg name="id" type="i" direction="in"/>
      <arg name="dev" type="s" direction="in"/>
      <arg name="point" type="s" direction="in"/>
      <arg name="res" type="b" direction="out"/>
    </method>
    <method name="unmounting">
      <arg name="id" type="i" direction="in"/>
      <arg name="point" type="s" direction="in"/>
      <arg name="res" type="b" direction="out"/>
    </method>
    <method name="setJobVisible" >
      <arg name="jobId" type="i" direction="in"/>
      <arg name="visible" type="b" direction="in"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
  </interface>
</node>
